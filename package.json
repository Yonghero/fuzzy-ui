{
  "name": "@hitotek/fuzzy-ui",
  "version": "1.0.11",
  "description": "fuzzy-ui",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "shx rm -rf lib types && rollup --config && tsc -p ./tsconfig.json",
    "publish": "pnpm run build && npm publish --access public",
    "gen": "bash ./scripts/gc.sh",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "types": "./types/index.d.ts",
  "keywords": [
    "ui"
  ],
  "exports": {
    ".": {
      "import": "./lib/components/index.mjs",
      "require": "./lib/components/index.cjs"
    },
    "./components": {
      "import": "./lib/components/index.mjs",
      "require": "./lib/components/index.cjs"
    },
    "./utils": {
      "import": "./lib/utils/index.mjs",
      "require": "./lib/utils/index.cjs"
    },
    "./style": {
      "import": "./lib/fuzzy-ui-style/index.css",
      "require": "./lib/fuzzy-ui-style/index.css"
    }
  },
  "peerdependencies": {
    "vue": "^3.2.26",
    "element-plus": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@hitotek/fuzzy-ui-components": "workspace:^1.0.0",
    "@hitotek/fuzzy-ui-utils": "workspace:^1.0.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@vitejs/plugin-vue": "2.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/babel-preset-jsx": "^1.4.0",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.14",
    "chalk": "4.1.2",
    "consola": "2.15.3",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.8",
    "escape-html": "^1.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.15.1",
    "hls.js": "^1.4.10",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "mpegts.js": "^1.7.3",
    "normalize.css": "^8.0.1",
    "pnpm": "^8.6.8",
    "postcss": "^8.4.26",
    "postcss-preset-env": "^9.1.1",
    "prismjs": "^1.29.0",
    "rollup": "^3.26.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.63.6",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "unplugin-element-plus": "^0.7.2",
    "video.js": "^8.3.0",
    "vite": "^4.4.4",
    "vitepress": "^1.0.0-beta.5",
    "vue": "^3.3.4"
  },
  "files": [
    "types",
    "lib"
  ],
  "author": "younghero",
  "license": "ISC",
  "dependencies": {
    "sortablejs": "^1.15.0"
  }
}
